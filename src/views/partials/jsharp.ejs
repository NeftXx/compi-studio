<div style="padding-bottom: 1em;">
  <div class="center-align" style="padding-top: 1em; padding-bottom: 1em;">
    <a id="btnNewBuffer" class="waves-effect waves-light btn-small brown lighten-2"><i
        class="material-icons right">insert_drive_file</i>Nuevo</a>
    <a id="btnUpFile" class="waves-effect waves-light btn-small blue"><i
        class="material-icons right">file_upload</i>Cargar</a>
    <a id="btnDownloadJSharp" class="waves-effect waves-light btn-small blue-grey lighten-1"><i
        class="material-icons right">file_download</i>Descargar código J#</a>
    <a id="btnDownloadC3D" class="waves-effect waves-light btn-small blue-grey"><i
        class="material-icons right">file_download</i>Descargar traducción</a>
    <a id="btnSendFiles" class="waves-effect waves-light btn-small"><i
        class="material-icons right">play_arrow</i>Traducir</a>
    <a id="btnClearTranslate" class="waves-effect waves-light btn-small red lighten-2"><i
        class="material-icons right">clear</i>Limpiar
      traducción</a>
  </div>
  <div class="row">
    <div class="input-field col s11">
      <select id="currentFile"> </select>
      <label>Archivos:</label>
    </div>
    <div class="col s1">
      <a id="btnClearBuffer" class="tooltipped btn-floating btn-small waves-effect waves-light grey lighten-1"
        data-position="bottom" data-tooltip="Limpiar"><i class="material-icons">clear</i></a>
      <a id="btnDeleteBuffer" class="tooltipped btn-floating btn-small waves-effect waves-light grey lighten-1"
        data-position="bottom" data-tooltip="Borrar"><i class="material-icons">delete</i></a>
    </div>
  </div>
  <div class="row">
    <div class="col s6">
      <textarea name="code" id="code"></textarea>
    </div>
    <div class="col s6">
      <textarea name="resultC3D" id="resultC3D"></textarea>
    </div>
  </div>
</div>
<div style="padding-top: 1em; padding-bottom: 2em;">
  <ul id="tabs-tables" class="tabs">
    <li class="tab col s3">
      <a class="active" href="#symbols-table">Tabla de simbolos</a>
    </li>
    <li class="tab col s3"><a href="#errors-table">Tabla de errores</a></li>
  </ul>
  <div id="symbols-table" class="col s12">
    <table class="highlight">
      <thead>
        <tr>
          <th>Tipo</th>
          <th>Descripción</th>
          <th>Fila</th>
          <th>Columna</th>
        </tr>
      </thead>

      <tbody id="body-symbols-table">
      </tbody>
    </table>
  </div>
  <div id="errors-table" class="col s12">
    <table class="highlight">
      <thead>
        <tr>
          <th>Tipo</th>
          <th>Descripción</th>
          <th>Fila</th>
          <th>Columna</th>
        </tr>
      </thead>
      <tbody id="body-errors-table">
      </tbody>
    </table>
  </div>
</div>