{
  "name": "compi-studio",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/NeftXx/compi-studio.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "copy:public": "ncp ./src/public/ ./dist/public/",
    "copy:ejs": "ncp ./src/views/ ./dist/views/",
    "build": "npm run clean && tsc && npm run copy:public && npm run copy:ejs",
    "tslint": "tslint --project tsconfig.json",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "jison:jsharp": "cd src/jsharp/grammar && jison grammar.jison grammar.jisonlex -o grammar.js",
    "jison:c3d": "cd src/intermediate_code/grammar && jison grammar.jison grammar.jisonlex -o grammar.js",
    "ts:node": "ts-node src/index.ts",
    "dev": "nodemon",
    "start": "node ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:write"
    }
  },
  "keywords": [],
  "author": "Ronald Berd√∫o",
  "license": "MIT",
  "dependencies": {
    "ejs": "^3.0.2",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "socket.io": "^2.3.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/morgan": "^1.9.0",
    "@types/socket.io": "^2.1.4",
    "husky": "^4.2.5",
    "jison": "^0.4.18",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}